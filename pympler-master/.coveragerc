[run]
branch = True
include = pympler/*
omit = pympler/util/bottle.py

[html]
directory = coverage_html_report

[report]
exclude_lines = 
    pragma: no cover
    except .*ImportError
    except .*NameError
    except .* # missing
    except .* # Python (3|2)
    if .* # Python 2.2
    if __name__ == '__main__'
    if sys.hexversion
    def __repr__
